  "sounds": {
    "schemeName": "macOS Monterey",
    "setFolder": "macos-monterey",
    "events": {
      "SystemAsterisk": "SystemAsterisk.wav",
      "SystemExclamation": "SystemExclamation.wav",
      "SystemNotification": "SystemNotification.wav",
      "DeviceConnect": "DeviceConnect.wav",
      "DeviceDisconnect": "DeviceDisconnect.wav",
      "MailBeep": "MailBeep.wav"
