  "sounds": {
    "schemeName": "Retro XP",
    "setFolder": "retro-xp",
    "events": {
      ".Default": "Default.wav",
      "SystemAsterisk": "SystemAsterisk.wav",
      "SystemExclamation": "SystemExclamation.wav",
      "SystemHand": "SystemHand.wav",
      "SystemNotification": "SystemNotification.wav",
      "SystemQuestion": "SystemQuestion.wav",
      "SystemExit": "SystemExit.wav",
      "WindowsLogon": "WindowsLogon.wav",
      "WindowsLogoff": "WindowsLogoff.wav",
